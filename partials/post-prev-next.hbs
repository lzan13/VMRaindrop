<div class="prev-next">
	<div class="prev-next-wrap">
		{{#prev_post}}
		<article class="prev-item">
			<span class="iconfont icon-arrow-left"></span><a href="{{url}}" class="title">{{title}}</a>
		</article>
		{{/prev_post}}

		{{#next_post}}
		<article class="next-item">
			<a href="{{url}}" class="title">{{title}}</a><span class="iconfont icon-arrot-right"></span>
		</article>
		{{/next_post}}
	</div>
</div>